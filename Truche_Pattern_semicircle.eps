%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 5 5 400 600
%%LanguageLevel: 2

%This is an attempt to produce an 2D tile
%based on empty square, filled square,
%a square filled with a quarter of a circle
%and a square filled with the complement of a
%square of a circle
%The tiles may be rotated with 90 degres


/q1{
    gsave
     0 0 5 0 90 arc
     0 0 lineto
     closepath
     1 0 0 setrgbcolor
     fill grestore
}def

/q2{
     gsave
     5 0 5 90 180 arc
     5 0 lineto
     closepath
     1 0 0 setrgbcolor
     fill grestore
}def

/q3{
     gsave
     5 5 5 180 270 arc
     5 5 lineto
     closepath
     1 0 0 setrgbcolor
     fill grestore
}def

/q4{
     gsave
     0 5 5 270 0 arc
     0 5 lineto
     closepath
     1 0 0 setrgbcolor
     fill grestore
}def

/sym [ //q1 //q2 //q3 //q4 ] def
/range sym length def
/rNum {rand range mod} def

%Initial setup
5 5 scale
0 setlinewidth
2 2 translate

%Start drawing

%horizontal boxes >> 15
%veritcal boxes >> 22

23 {

  14 {
    % select a random symbol to be drawn
    sym rNum get exec
    5 0 translate
  } repeat

  %go up one row
  -70 5 translate
} repeat


showpage
%%EOF
