%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 5 5 400 600
%%LanguageLevel: 2

%This is an attempt to produce an 2D tile
%based on circles and corners
%two circles and two corners
%diagonals and up, down, left, right

/q1{
    gsave
    5 5 2.5 180 270 arc stroke
    0 2.5 moveto
    2.5 -2.5 rlineto
    stroke
    grestore
}def

/q2{
   gsave
   5 0 translate 90 rotate q1
   grestore
}def

/q3{
   gsave
   5 5 translate 180 rotate q1
   grestore
}def

/q4{
   gsave
   0 5 translate -90 rotate q1 
   grestore
}def

/sym [ //q1 //q2 //q3 //q4 ] def
/range sym length def
/rNum {rand range mod} def

%Initial setup
5 5 scale
0.4 setlinewidth
2 2 translate

%Start drawing

%horizontal boxes >> 15
%veritcal boxes >> 22

23 {

  14 {
    % select a random symbol to be drawn
    sym rNum get exec
    5 0 translate
  } repeat

  %go up one row
  -70 5 translate
} repeat


showpage
%%EOF
